<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.psl.pallettracking">

    <uses-sdk
        android:minSdkVersion="24"
        android:targetSdkVersion="28"
        tools:overrideLibrary="com.android.car.ui" />

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.SEUICAsset">
        <activity
            android:name=".TruckMappingForDispatchActivity"
            android:exported="false" />
        <activity
            android:name=".ItemMovementActivity"
            android:exported="false"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".PalletMovementActivity"
            android:exported="false" />
        <activity
            android:name=".BinPartialPalletMapProcessActivity"
            android:exported="false" />
        <activity
            android:name=".BinPartialPalletMappingActivity"
            android:exported="false" />
        <activity
            android:name=".AssetPalletWithItemActivity"
            android:exported="false" />
        <activity
            android:name=".TruckMappingActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AssetPalletMappingActivity"
            android:exported="false"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".PalletContainerMappingActivity"
            android:exported="false"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".DummyBarcodeScannerActivity"
            android:exported="false"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".URLConfigActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AssetMappingActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AssetSearchActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AssetInventoryActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".DashboardActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".LoginActivity"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>